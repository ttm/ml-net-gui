{
  "name": "va2",
  "devDependencies": {
    "standard": "*"
  },
  "scripts": {
    "build": "browserify scripts/main.js > scripts/bundle.js",
    "dev": "nodemon app.js --exec 'standard && npm run build && node'",
    "buildAux": "browserify you/contentScript.js > you/contentScript_ok.js",
    "buildAux2": "browserify you/background.js > you/background_ok.js",
    "buildAux3": "browserify you/pop.js > you/pop_ok.js",
    "buildEauto2": "nodemon app.js --exec 'standard && npm run build && npm run buildAux && npm run buildAux2 && npm run buildAux3 && node'",
    "buildE2": "standard you/pop.js you/contentScript.js you/background.js you/fnetwork.js && npm run buildAux && npm run buildAux2 && browserify you/pop.js > you/pop_ok.js",
    "fixme": "standard --fix"
  },
  "dependencies": {
    "@eastdesire/jscolor": "^2.4.0",
    "@fortawesome/fontawesome-free": "^5.13.1",
    "browserify": "^17.0.0",
    "chroma-js": "^2.1.0",
    "color-scheme": "^1.0.1",
    "copy-to-clipboard": "^3.3.1",
    "dat.gui": "^0.7.7",
    "distinct-colors": "^3.0.0",
    "encodeurl": "^1.0.2",
    "fast-sha256": "^1.3.0",
    "flatpickr": "^4.6.6",
    "graphology": "^0.17.1",
    "graphology-communities-louvain": "^1.2.0",
    "graphology-components": "^0.1.0",
    "graphology-generators": "^0.10.1",
    "graphology-layout": "^0.2.0",
    "graphology-layout-forceatlas2": "^0.4.4",
    "graphology-metrics": "^1.8.0",
    "graphology-types": "^0.19.2",
    "graphology-utils": "^1.8.0",
    "jquery": "^3.5.1",
    "linkifyjs": "^2.1.9",
    "lz-string": "^1.4.4",
    "mongodb-stitch-browser-sdk": "^4.8.0",
    "nodemon": "^2.0.3",
    "nosleep.js": "^0.12.0",
    "paginationjs": "^2.1.5",
    "percom": "^1.1.0",
    "pixi-projection": "^0.3.11",
    "pixi.js": "^5.3.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "scribbletune": "^3.15.0",
    "showdown": "^1.9.1",
    "stats-js": "^1.0.1",
    "superagent": "^6.1.0",
    "tinycolor2": "^1.4.1",
    "tone": "^14.7.77",
    "uglify-js": "^3.13.9",
    "wavefile": "^11.0.0"
