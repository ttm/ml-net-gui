{
 "manifest_version": 2,
 "name": "Hidra grabber",
 "version": "0.01",

 "permissions": [
   "activeTab"
 ],

 "browser_action": {
    "default_icon": "nother.png",
    "default_popup": "pop.html",
    "default_title": "Hydra grabber for the Hydra@OA initiative"
  },

  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com*"
      ],
      "js": ["scripts/fb_scrape.js"]
    }
  ]
}
